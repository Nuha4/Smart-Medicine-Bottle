      <div class="container">
        <div class="panel panel-default">
            <!-- Default panel contents -->
            <div class="panel-heading"> Weight of Medicine  and Time</div>
            <div class="panel-body">
            </div>
          
            <!-- Table -->
            <table class="table">
                <thead>
                    <tr>
                      <th>Sl. No</th>
                      <th>Weight</th>
                      <th>Date & Time</th>
                      
                    </tr>
                  </thead>
                  <tbody>
                      <% var j=1; %>
                      <% for(var i=0; i<CellData.length; i++) {%>
                          <tr>
                            <td>
                              <%= j++ %>
                            </td>
                            <td>
                              <%= CellData[i].value %>
                            </td>
                            <td>
                              <%= CellData[i].DateTime %>
                            </td>
                          </tr>
                          <% } %>
                </tbody>
            </table>
        </div>
      </div>



      <div class="container">
        <h2>Table</h2>
          <div class="table-responsive">
            <div class="col-md-12">
                <div class="col-md-6">              
                    <table class="table" style="border:2px solid blue;">
                      <thead>
                        <tr>
                          <th>Sl. No</th>
                          <th>Firstname</th>
                          <th>Lastname</th>
                          
                        </tr>
                      </thead>
                      <tbody>
                          <% var j=1; %>
                          <% for(var i=0; i<CellData.length; i++) {%>
                              <tr>
                                <td>
                                  <%= j++ %>
                                </td>
                                <td>
                                  <%= CellData[i].value %>
                                </td>
                                <td>
                                  <%= CellData[i].DateTime %>
                                </td>
                              </tr>
                              <% } %>
                     </tbody>
                  </table>
                </div>
             </div>
        </div>
      </div>

</body>
